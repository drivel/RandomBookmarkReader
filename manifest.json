{
    "background": {
        "scripts": ["scripts/background.js"],
        "persistent": true
      },
    "browser_action": {
       "default_icon": {
          "128": "images/icon128.png",
          "16": "images/icon16.png",
          "32": "images/icon32.png",
          "48": "images/icon48.png"
       },
       "default_popup": "popup.htm",
       "default_title": "Random Bookmark Reader"
    },
    "description": "Random Bookmark Reader is a fork of the defunct Local Read Later extension for Chrome.",
    "icons": {
       "128": "images/icon128.png",
       "16": "images/icon16.png",
       "32": "images/icon32.png",
       "48": "images/icon48.png"
    },
    "key": "",
    "manifest_version": 2,
    "name": "Random Bookmark Reader",
    "options_ui": {
        "chrome_style": true,
        "page": "options.htm"
      },
    "permissions": [ "bookmarks", "chrome://favicon/", "tabs" ],
    "version": "0.1"
 }